thraneSoftwareDownload OBJECT IDENTIFIER ::= { thraneProductsGeneric 2 }

thraneSoftwareDownloadTraps OBJECT IDENTIFIER ::= { thraneSoftwareDownload 0 }

ttSoftwareDownloadProtocol OBJECT-TYPE
    SYNTAX INTEGER
    {
        tftp(1)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Defines which protocol should be used to the software download
            ~D0R0W0P0S(Software Download Protocol)G(none)"
    DEFVAL { 1 }
::= { thraneSoftwareDownload 1 }

ttSoftwareDownloadIpAddress OBJECT-TYPE
    SYNTAX IpAddress
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "IP-Address of the TFTP server
            ~D0R0W0P0S(Software Download IP)G(none)"
::= { thraneSoftwareDownload 2 }

ttSoftwareDownloadPortNumber OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Port number of the TFTP server. Default value is 69
            ~D0R0W0P0S(Software Download Port)G(none)"
    DEFVAL { 69 }
::= { thraneSoftwareDownload 3 }

ttSoftwareDownloadFileName OBJECT-TYPE
    SYNTAX DisplayString
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "The name of the file to retrieve from the TFTP server
            ~D0R0W0P0S(Software Download Filename)G(none)"
::= { thraneSoftwareDownload 4 }

ttSoftwareDownloadControl OBJECT-TYPE
    SYNTAX INTEGER
    {
        stop(1),
        start(2)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "1: Stop (abort) file transfer
         2: Start the file transfer
         A read of this parameter returns the last written value
            ~D0R0W0P0S(Software Download Control)G(none)"
    DEFVAL { 1 }
::= { thraneSoftwareDownload 5 }

ttSoftwareDownloadStatus OBJECT-TYPE
    SYNTAX INTEGER
    {
        init(1),
        started(2),
        aborted(3),
        failed(4),
        ready(5)
    }
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Status of file transfer
            ~D1R0W0P0S(Software download status)G(thranePropertyFolder3)"
::= { thraneSoftwareDownload 6 }

ttSoftwareDownloadErrorDescription OBJECT-TYPE
    SYNTAX DisplayString
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Textual description of the reason for an error that occurred 
         during the software download
            ~D1R0W0P0S(Software download error description)G(thranePropertyFolder3)"
::= { thraneSoftwareDownload 7 }

ttSoftwareDownloadProgress OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Progress status of the software download, range: 0-100%.
         If this feature is unsupported, the progress value should be -1
            ~D1R0W0P0S( System property)G(none)"
::= { thraneSoftwareDownload 8 }

ttSoftwareDownloadTrap NOTIFICATION-TYPE
    OBJECTS
    {
        ttSoftwareDownloadStatus
    }
    STATUS current
    DESCRIPTION 
        "This (optional) trap is sent, if the software download status changes"
::= { thraneSoftwareDownloadTraps 1 }


