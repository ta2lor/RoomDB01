<script type="text/javascript">
blocking_zones = {"zones":[{"enabled":1,"notx":1,"azimuth":{"start":{"val":45},"stop":{"val":135}},"elevation":{"start":{"val":0},"stop":{"val":80}}},{"enabled":0,"notx":0,"azimuth":{"start":{"val":0},"stop":{"val":0}},"elevation":{"start":{"val":0},"stop":{"val":0}}},{"enabled":0,"notx":0,"azimuth":{"start":{"val":0},"stop":{"val":0}},"elevation":{"start":{"val":0},"stop":{"val":0}}},{"enabled":0,"notx":0,"azimuth":{"start":{"val":0},"stop":{"val":0}},"elevation":{"start":{"val":0},"stop":{"val":0}}},{"enabled":0,"notx":0,"azimuth":{"start":{"val":0},"stop":{"val":0}},"elevation":{"start":{"val":0},"stop":{"val":0}}},{"enabled":0,"notx":0,"azimuth":{"start":{"val":0},"stop":{"val":0}},"elevation":{"start":{"val":0},"stop":{"val":0}}},{"enabled":0,"notx":0,"azimuth":{"start":{"val":0},"stop":{"val":0}},"elevation":{"start":{"val":0},"stop":{"val":0}}},{"enabled":0,"notx":0,"azimuth":{"start":{"val":0},"stop":{"val":0}},"elevation":{"start":{"val":0},"stop":{"val":0}}}],"min_ele":-90,"max_ele":90,"img":"/images/zones.png","notx":1,"ship_azi":255320,"ship_ele":41420};

function dashboard_cb(data)
{
    if (!("dashboard" in data))
        return;

    var db_data = data.dashboard;

    for (it in db_data)
        $("#" + it).html(db_data[it]);

    if ("dm_role" in db_data)
    {
        $("#dm_role_row").show();
        $("#dm_status_row").show();
    }
    else
    {
        $("#dm_role_row").hide();
        $("#dm_status_row").hide();
    }

    if ("dm_peer_ip" in db_data)
    {
        var url = db_data["dm_peer_ip"];
        var text = db_data["dm_link_text"];
        $("#dm_link").html("<a href='http://" + window.location.hostname + url + "'>" + text + "</a>");
    }
}

register_status_cb({"fun": dashboard_cb, "name": "dashboard"});
</script>
